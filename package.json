{
  "name": "naelBirthday",
  "private": true,
  "version": "0.0.1",
  "type": "commonjs",
  "homepage": "./",
  "scripts": {
    "version:patch": "node push-new-version.js patch",
    "version:minor": "node push-new-version.js minor",
    "start": "npx cross-env VITE_IS_DEV_SPLASH=true npx ionic serve",
    "sync": "npx cap sync",
    "build": "vite build --base=./",
    "build:android": "npx ionic capacitor build android",
    "build:electron": "npx ionic build && npx cap copy @capacitor-community/electron",
    "start:electron": "npm run build:electron && cd electron && npm run electron:start",
    "install:android": "set \"JAVA_HOME=C:/Program Files/java/jdk-21.0.7\" && npx ionic capacitor run android",
    "open:android": "npx cap open android",
    "copy:apk": "cp ./android/app/build/outputs/apk/release/* ./apks/",
    "build:apk": "npm run build:android && cd android && ./gradlew assembleRelease",
    "windows-build:apk": "npm run build:android && cd android && gradlew.bat assembleRelease",
    "upgrade-android-version-code": "node push-new-bundle-version.js",
    "build:bundle": "npm run build && npx cap sync android && cd android && ./gradlew :app:bundleRelease",
    "windows-build:bundle": "npm run build && npx cap sync android && cd android && gradlew.bat :app:bundleRelease",
    "build:electron-exe": "cd electron && npm run electron:make"
  },
  "dependencies": {
    "@awesome-cordova-plugins/android-full-screen": "^6.4.0",
    "@capacitor/android": "^6.2.0",
    "@capacitor/app": "6.0.2",
    "@capacitor/core": "6.2.0",
    "@capacitor/device": "6.0.2",
    "@capacitor/haptics": "6.0.2",
    "@capacitor/keyboard": "6.0.3",
    "@capacitor/local-notifications": "^6.1.1",
    "@capacitor/preferences": "^6.0.3",
    "@capacitor/status-bar": "6.0.2",
    "@capawesome/capacitor-background-task": "^6.0.0",
    "@ionic/react": "^7.4.0",
    "@ionic/react-router": "^7.0.0",
    "@types/react-router": "^5.1.20",
    "@types/react-router-dom": "^5.3.3",
    "cordova-plugin-fullscreen": "^1.3.0",
    "howler": "^2.2.4",
    "ionicons": "^7.0.0",
    "phaser": "^3.90.0",
    "platform": "^1.3.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router": "^5.3.4",
    "react-router-dom": "^5.3.4",
    "seedrandom": "^3.0.5",
    "three": "^0.157.0"
  },
  "devDependencies": {
    "@capacitor/cli": "^5.6.0",
    "@ionic/cli": "^7.2.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/electron": "^1.6.12",
    "@types/howler": "^2.2.11",
    "@types/platform": "^1.3.4",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@types/seedrandom": "^3.0.8",
    "@types/three": "^0.157.2",
    "@vitejs/plugin-legacy": "^4.0.2",
    "@vitejs/plugin-react": "^4.0.1",
    "cypress": "^12.7.0",
    "electron": "^32.2.8",
    "electron-updater": "^6.3.9",
    "eslint": "^8.49.0",
    "eslint-plugin-react": "^7.32.2",
    "jsdom": "^22.1.0",
    "prettier": "^3.3.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.1.6",
    "vite": "4.5.3",
    "vitest": "^0.32.2"
  },
  "workspaces": [
    "electron"
  ],
  "description": "An Ionic project"
}
